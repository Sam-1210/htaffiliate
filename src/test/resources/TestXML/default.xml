<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="htaffiliate" thread-count="4" data-provider-thread-count="2">
    <listeners>
        <listener class-name="org.shopnow.listeners.TestListener"/>
    </listeners>

    <parameter name="environment" value="prod"/>
    <parameter name="executionType" value="sanity"/>
    <parameter name="platformName" value="web"/>
    <parameter name="browser" value="chrome"/>
    <parameter name="browserMode" value="headless"/>
    <parameter name="isBrowserStack" value="false"/>
    <parameter name="isAppium" value="false"/>
    <parameter name="capabilitiesJson" value="windows10_chrome"/>
    <parameter name="buildNumber" value="12302"/>
    <parameter name="googleSheetFlag" value="false"/>
    <test name="Test">
        <classes>
            <class name="org.shopnow.tests.HomePage"/>
        </classes>
    </test>
</suite>
